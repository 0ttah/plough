{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://playartifact.com/cardset/",
  "type": "object",
  "title": "Root cardset schema",
  "required": [
    "cdn_root",
    "url",
    "expire_time"
  ],
  "properties": {
    "cdn_root": {
      "$id": "#/properties/cdn_root",
      "type": "string",
      "title": "CDN root host",
      "default": "",
      "examples": [
        "https://domain-name.net/"
      ],
      "pattern": "^(.*)$"
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "URL path to set JSON file",
      "default": "",
      "examples": [
        "/path/to/set.json"
      ],
      "pattern": "^(.*)$"
    },
    "expire_time": {
      "$id": "#/properties/expire_time",
      "type": "integer",
      "title": "Cache expire time",
      "default": 0,
      "examples": [
        1542024260
      ]
    }
  }
}